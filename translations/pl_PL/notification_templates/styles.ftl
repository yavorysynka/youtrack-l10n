<#assign event_value = "padding: 10px 0 10px 10px; vertical-align: top; font-family:sans-serif; font-size:13px;">
<#assign new_value = "background-color: #ffffc4; text-decoration: none!important; font-family: sans-serif; font-size:13px;">
<#assign old_value = "color: #888888; text-decoration: line-through!important; font-family:sans-serif; font-size:13px;">
